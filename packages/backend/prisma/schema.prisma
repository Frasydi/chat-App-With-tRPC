// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model User {
  id Int @id @default(autoincrement())
  uuid String @unique
  username String @unique
  password String
}

model Chat {
  id Int @id @default(autoincrement())
  from String 
  to String
  text String?
  image String? 
  date DateTime 
}


